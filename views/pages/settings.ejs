</head>
<body>
    <%- include('../partials/cookies', {cookies: cookies}) %>
    <%- include('../partials/header', { username: username }) %>
    <div id="hydrauliscselapp">
        <div class="damagepackage">
            <div class="setting-contents">
                <div class="dpkg-setting-contents">
                    <main id="maincontent" style="min-width: 0px;">
                        <div class="sticky-container" tabindex="-1">
                            <div class="sticky-contdiv">
                                <div class="forbiddenbkb">
                                    <div class="forbiddencontent">
                                        <div>
                                            <div class="forbiddenwide">
                                                <div class="settingsrednav">
                                                    <div>
                                                        <nav class="settingsnav">
                                                            <section class="group">
                                                                <div class="setnav-title">User Settings</div>
                                                                <div>
                                                                    <a onclick="profileSettings()" class="nav_button">
                                                                        <span class="text">Profile</span>
                                                                    </a>
                                                                    <a onclick="privacySettings()" class="nav_button">
                                                                        <span class="text">Privacy</span>
                                                                    </a>
                                                                    <a onclick="notifSettings()" class="nav_button">
                                                                        <span class="text">Notifications</span>
                                                                    </a>
                                                                    <a onclick="mailNotifications()" class="nav_button">
                                                                        <span class="text">Email</span>
                                                                    </a>
                                                                    <a onclick="securitySettings()" class="nav_button">
                                                                        <span class="text">Security</span>
                                                                    </a>
                                                                </div>
                                                            </section>
                                                            <section class="group">
                                                                <div class="setnav-title">Client Settings</div>
                                                                <div>
                                                                    <a onclick="generalClient()" class="nav_button">
                                                                        <span class="text">General</span>
                                                                    </a>
                                                                    <a onclick="themeFunction()" class="nav_button">
                                                                        <span class="text">Theme</span>
                                                                    </a>
                                                                    <a onclick="pluginsList()" class="nav_button">
                                                                        <span class="text">Plugins</span>
                                                                    </a>
                                                                </div>
                                                            </section>
                                                            <section class="group">
                                                                <div class="setnav-title">Other Settings</div>
                                                                <div>
                                                                    <a onclick="mute_or_unmute()" class="nav_button">
                                                                        <span class="text">Mutes and Blocks</span>
                                                                    </a>
                                                                    <a onclick="apiSetting()" class="nav_button">
                                                                        <span class="text">API</span>
                                                                    </a>
                                                                    <a onclick="payloadUrl()" class="nav_button">
                                                                        <span class="text">Webhook</span>
                                                                    </a>
                                                                    <a onclick="additionalSettings()" class="nav_button">
                                                                        <span class="text">Other</span>
                                                                    </a>
                                                                </div>
                                                            </section>
                                                            <section class="group">
                                                                <div>
                                                                    <button onclick="handleLogout()" class="danger" style="border: none; border-radius: 6px; padding: .65rem; cursor: pointer; width: 100%; margin-top: .35rem;">
                                                                        <span class="text">Sign Out</span>
                                                                    </button>
                                                                </div>
                                                            </section>
                                                        </nav>
                                                    </div>
                                                </div>
                                                <section class="mainsettab">
                                                    <div id="profiletabulatordisplay" style="display: block;" class="tobesetnone">
                                                        <div class="_formRoot" tabindex="-1">
                                                            <br>
                                                            <div class="lluviere" style="background-image: url('<%- banner %>');">
                                                                <div class="avatar">
                                                                    <span class="squircle-avatar" style="color: #1C286B;" title="Avatar">
                                                                        <img class="inner" src="<%- pfp %>" loading="lazy">
                                                                    </span>
                                                                    <button id="avatarEdit" onclick="changepfpmodal()">
                                                                        <div>Change avatar</div>
                                                                    </button>
                                                                </div>
                                                                <button id="bannerEdit" onclick="changebannermodal()">
                                                                    <div>Change banner</div>
                                                                </button>
                                                            </div>
                                                            <div class="nameForm">
                                                                <label>
                                                                    <div class="label">Display Name</div>
                                                                    <div class="input">
                                                                        <input spellcheck="false" value="<%- username %>" style="border-color: rgb(19, 19, 19); cursor: not-allowed !important;" disabled max="30" maxlength="30">
                                                                    </div>
                                                                </label>
                                                            </div>
                                                            <div class="biographyForm">
                                                                <label>
                                                                    <span class="label">Bio</span>
                                                                    <div class="input">
                                                                        <% if (userBiography) { %>
                                                                            <textarea id="thisistheuserbioid" spellcheck="false" style="border-color: rgb(19, 19, 19); border-radius: 8px 8px 0 8px;" maxlength="500" max="500" value="<%- userBiography %>"><%- userBiography %></textarea>
                                                                        <% } else { %>
                                                                            <textarea id="thisistheuserbioid" spellcheck="false" style="border-color: rgb(19, 19, 19); border-radius: 8px 8px 0 8px;" maxlength="500" max="500"></textarea>
                                                                        <% } %>
                                                                    </div>
                                                                    <div class="caption">You can also include hash's in your bio.</div>
                                                                </label>
                                                                <button onclick="saveSayonara()" class="button_save">
                                                                    <div> Save</div>
                                                                </button>
                                                            </div>
                                                            <div class="matxzzsk _formBlock" style="--a8ff023a: 38px;">
                                                                <label>
                                                                    <div class="label">Birthday (Optional!)</div>
                                                                    <div class="input">
                                                                        <input type="date" disabled style="border-color: rgb(19, 19, 19);">
                                                                    </div>
                                                                    <div class="caption">Birthday's aren't (currently) used for anything. We might add a <span style="font-family: monospace; color: white; background: black;">{{event.birthday}}</span> in the future.</div>
                                                                </label>
                                                                <!--
                                                                <button onclick="saveSayonara()" class="button_save">
                                                                    <div> Save</div>
                                                                </button>
                                                                -->
                                                            </div><br><br>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox" disabled>
                                                                <span class="label">
                                                                    <span>Mark this account as automated</span>
                                                                    <p class="caption"><b>WARNING! EXPERIMENTAL!</b> Enable this option if this account is controlled by a program. If enabled, it will act as a flag for other developers to prevent endless interaction chains with other automated accounts and adjust Hydraulisc's internal systems to treat this account as an automated account.</p>
                                                                </span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div id="privacytabulatordisplay" style="display: none;" class="tobesetnone">
                                                        <div class="_formRoot">
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox">
                                                                <span class="label">
                                                                    <span>Follow requests require approval</span>
                                                                    <p class="caption">Unless you set your profile visiblity to "Followers only", your posts will be visible to anyone, even if you require followers to be manually approved.</p>
                                                                </span>
                                                            </label>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox" checked>
                                                                <span class="label">
                                                                    <span>Set comments history to public </span>
                                                                    <p class="caption">This will make the list of all your past comments publicly visible.</p>
                                                                </span>
                                                            </label>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox">
                                                                <span class="label">
                                                                    <span>Allow Private Messages from Unknown Users</span>
                                                                    <p class="caption">If this is unchecked, you can only PM someone who you mutually follow, even if you don't require followers to be manually approved.</p>
                                                                </span>
                                                            </label>
                                                            <div class="vblkjoeq _formBlock" style="--59f2add9: 36px;">
                                                                <div class="label">Profile Visibility</div>
                                                                <div class="input">
                                                                    <div class="prefix"></div>
                                                                    <select class="select" style="border-color: rgb(35, 35, 47); padding-right: 34px;">
                                                                        <option value="public">Public</option>
                                                                        <option value="followers">Visible to followers only</option>
                                                                        <!-- <option value="private">Private</option> -->
                                                                    </select>
                                                                    <p class="caption" style="font-size: smaller !important;">Allows you to configure who can see your profile.</p>
                                                                </div>
                                                            </div>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox" disabled>
                                                                <span class="label">
                                                                    <span>Indexable </span>
                                                                    <p class="caption">Allow built-in search to show your public posts</p>
                                                                </span>
                                                            </label>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox" disabled>
                                                                <span class="label">
                                                                    <span>Reject crawler indexing </span>
                                                                    <p class="caption">Ask external search engines to not index your content.</p>
                                                                </span>
                                                            </label>
                                                            <label class="ziffeomt">
                                                                <input type="checkbox" checked disabled>
                                                                <span class="label">
                                                                    <span>Prevent AI bot scraping <sup title="This is a very Beta feature!" style="text-decoration: underline;">Beta</sup></span>
                                                                    <p class="caption">Request third-party AI language models not to study content you upload, such as posts and images.</p>
                                                                </span>
                                                            </label>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox">
                                                                <span class="label">
                                                                    <span>Disable "Recommendations" </span>
                                                                    <p class="caption">If you turn this off, you will not receive random recommendations, but your account will not show up in the "Recommendations" sections either.</p>
                                                                </span>
                                                            </label>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox" checked disabled>
                                                                <span class="label">
                                                                    <span>Keep content warnings</span>
                                                                    <p class="caption">This will remind you to accept cookies more often and <b>push more content warnings!</b></p>
                                                                </span>
                                                            </label>
                                                            <label class="ziffeomt _formBlock">
                                                                <input type="checkbox">
                                                                <span class="label">
                                                                    <span>Add "re:" at the beginning of comments and replies.</span>
                                                                    <p class="caption"></p>
                                                                </span>
                                                            </label>
                                                            <button class="button_save">
                                                                <div> Save</div>
                                                            </button>
                                                        </div>
                                                    </div>
                                                    <div id="notificationstabulatordisplay" style="display: none;" class="tobesetnone">
                                                        <div class="_formRoot">
                                                            <h3>Notifications aren't currently configured on this client!</h3>
                                                        </div>
                                                    </div>
                                                    <div id="emailtabulatordisplay" style="display: none;" class="tobesetnone">
                                                        <div>
                                                            <div class="_formRoot">
                                                                <section class="_formBlock" style="border-bottom: .5px solid rgb(35, 35, 47);">
                                                                    <h3 class="label">Email</h3>
                                                                    <div class="_formRoot">
                                                                        <div>
                                                                            <label>
                                                                                <div class="input" style="position: relative;">
                                                                                    <input type="email" value="support@hydraulisc.net" disabled style="cursor: not-allowed;">
                                                                                    <div class="suffix">Use the support email above to request a change to your email. A verification code will appear here for 7 days after we process your request to ensure it's you requesting the change.</div>
                                                                                </div>
                                                                                <p style="background: black; color: white; font-family: monospace;">{{email.verified}}</p>
                                                                                <button class="button_save" style="display: none !important;">
                                                                                    <div> Save</div>
                                                                                </button>
                                                                            </label>
                                                                        </div>
                                                                    </div>
                                                                </section>
                                                                <section class="_formBlock" style="border-bottom: .5px solid rgb(35, 35, 47);">
                                                                    <h3 class="label"></h3>
                                                                    <div class="_formRoot">
                                                                        <label>
                                                                            <input type="checkbox" checked disabled>
                                                                            <span class="label">
                                                                                <span style="background: black; color: white; font-family: monospace;">{{event.notificationreceive}}</span>
                                                                                <p class="caption">Notifications aren't currently supported on this client!</p>
                                                                            </span>
                                                                        </label>
                                                                    </div>
                                                                </section>
                                                                <section class="_formBlock">
                                                                    <h3 class="label">Email Notifications</h3>
                                                                    <div class="_formRoot">
                                                                        <label>
                                                                            <span class="label">
                                                                                <p class="caption">We only send important security emails, such as Password Reset Requests or Password/Email Changes.</p>
                                                                            </span>
                                                                        </label>
                                                                    </div>
                                                                </section>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div id="securitytabulatordisplay" style="display: none;" class="tobesetnone">
                                                        <div class="_formRoot" style="outline: none;">
                                                            <section class="_formBlock" style="padding-bottom: 1rem; border-bottom: .5px solid rgb(35, 35, 47);">
                                                                <h3 class="label">Password</h3>
                                                                <div class="_formRoot">
                                                                    <button class="danger" style="border: none; padding: .65rem; cursor: pointer; border-radius: 8px; color: #d7d9e0 !important;">
                                                                        <div> Change Password</div>
                                                                    </button>
                                                                </div>
                                                            </section>
                                                            <section class="_formBlock" style="border-bottom: .5px solid rgb(35, 35, 47); padding-bottom: 1rem;">
                                                                <h3 class="label">MFA</h3>
                                                                <div class="_formRoot">
                                                                    <label>
                                                                        <button style="background: crimson; font-weight: 700; border: none; padding: .65rem; cursor: not-allowed; border-radius: 8px; color: #d7d9e0 !important;" disabled>
                                                                            <div> Add Authenticator</div>
                                                                        </button>
                                                                        <button style="background: crimson; font-weight: 700; border: none; padding: .65rem; cursor: not-allowed; border-radius: 8px; color: #d7d9e0 !important;" disabled>
                                                                            <div> Register Pass Key</div>
                                                                        </button>
                                                                        <span class="label">
                                                                            <p class="caption">MFA is not currently supported. Please come back later.</p>
                                                                            <!--<p class="caption">Add an extra layer of security by enabling 2FA on your account.</p>-->
                                                                        </span>
                                                                    </label>
                                                                </div>
                                                            </section>
                                                            <section class="_formBlock" style="display: none !important;">
                                                                <h3 class="label">Sign In History</h3>
                                                                <!-- Neither the API nor the backend is ready for this. Please come back later :-) -->
                                                            </section>
                                                            <secion class="_formBlock">
                                                                <h3 class="label"></h3>
                                                                <div class="_formRoot">
                                                                    <label>
                                                                        <button onclick="manualRefreshTokens()" class="danger" style="border: none; padding: .65rem; cursor: pointer; border-radius: 8px; color: #d7d9e0 !important;">
                                                                            <div> Regenerate Account Tokens</div>
                                                                        </button>
                                                                        <span class="label">
                                                                            <p class="caption">Regenerates all tokens used internally during sign in. Normally this action is not necessary. If regenerated, all devices will be logged out.</p>
                                                                        </span>
                                                                    </label>
                                                                </div>
                                                            </secion>
                                                        </div>
                                                    </div>
                                                    <div id="generaltabulatordisplay" style="display: none;" class="tobesetnone">
                                                        <div class="_formRoot" style="outline: none;">
                                                            <div class="_formBlock">
                                                                <div class="label">Interface Language</div>
                                                                <div class="input">
                                                                    <select class="select" style="border-color: rgb(35, 35, 47); padding-right: 34px;">
                                                                        <option value="en-EN">English</option>
                                                                        <option value="es-ES">Español</option>
                                                                        <option value="ca-ES">Català</option>
                                                                    </select>
                                                                </div>
                                                                <div class="caption">
                                                                    <span class="label">
                                                                        <p class="caption">Hydraulisc is undergoing some major modifications. Translations are a very WIP feature, and do not currently save. Please be patient, weblate translations coming soon! <span style="display: none !important;">You can help translate at <a href="">Weblate</a></span></p>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                            <fieldset class="_formBlock" style="padding-bottom: 1rem; border-bottom: .5px solid rgb(35, 35, 47);">
                                                                <legend class="caption">Icon Set</legend>
                                                                <div>
                                                                    <label style="border-color: rgba(255, 89, 117, 0.15);">
                                                                        <input type="radio" checked>
                                                                        <span class="button">
                                                                            <span></span>
                                                                        </span>
                                                                        <span class="label">Default</span>
                                                                    </label>
                                                                </div>
                                                            </fieldset>
                                                        </div>
                                                    </div>
                                                    <div id="themestabulatordisplay" style="display: none;" class="tobesetnone">
                                                        <section class="main">
                                                            <div>
                                                                <div class="_formRoot">
